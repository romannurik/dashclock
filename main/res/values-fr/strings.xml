    <!-- Weather -->
    <string name="weather_extension_title">Météo</string>
    <string name="weather_extension_description">
        Affiche les informations météo les plus récentes près de vous, incluant la température et 
        les conditions courantes, ainsi que les prévisions. Fourni par Yahoo! Weather.
    </string>
    <string name="title_weather_settings">Paramètres de l\'extension Météo</string>
    <string name="temperature_template">%1$s°</string>
    <string name="later_forecast_template">Plus tard : %1$s</string>
    <string name="no_weather_data">Aucune donnée météorologiques disponible</string>
    <string name="no_location_data">Votre position ne peut pas être déterminée</string>

    <!-- Gmail -->
    <string name="gmail_extension_title">Gmail</string>
    <string name="gmail_extension_description">
        Affiche un compteur de courriels non lus pour la boîte de réception classique ou prioritaire de Gmail.
    </string>
    <string name="title_gmail_settings">Paramètres de l\'extension Gmail</string>
    <plurals name="gmail_title_template">
        <item quantity="one">%1$d non lu</item>
        <item quantity="other">%1$d non lus</item>
    </plurals>

    <!-- Next alarm -->
    <string name="next_alarm_extension_title">Prochaine alarme</string>
    <string name="next_alarm_extension_description">
        Affiche les informations pour les prochaines alarmes planifiées, si vous en avez configurées.
    </string>
    <string name="next_alarm">Prochaine alarme</string>

    <!-- Calendar -->
    <string name="calendar_extension_title">Rendez-vous du Calendrier</string>
    <string name="calendar_extension_description">
        Affiche les informations à propos des prochains rendez-vous du calendrier, s\'ils sont planifiés dans 
        les prochaines heures.
    </string>
    <string name="title_calendar_settings">Paramètres de l\'extension Calendrier</string>
    <plurals name="calendar_template_days">
        <item quantity="one">%1$s jour</item>
        <item quantity="other">%1$s jours</item>
    </plurals>
    <plurals name="calendar_template_hours">
        <item quantity="one">%1$s heure</item>
        <item quantity="other">%1$s heures</item>
    </plurals>
    <plurals name="calendar_template_mins">
        <item quantity="other">%1$s min</item>
    </plurals>

    <!-- Missed calls -->
    <string name="missed_calls_extension_title">Appels manqués</string>
    <string name="missed_calls_extension_description">
        Affiche le nombre d\'appels manqués et le nom des appelants que vous avez manqués.
    </string>
    <plurals name="missed_calls_title_template">
        <item quantity="one">%1$s appel manqué</item>
        <item quantity="other">%1$s appels manqués</item>
    </plurals>
    <string name="missed_calls_body_template">De : %1$s</string>

    <!-- SMS + MMS -->
    <string name="sms_extension_title">SMS</string>
    <string name="sms_extension_description">
        Affiche le nombre de conversations SMS et MMS non lus , avec le nom de l\'expéditeur.
    </string>
    <plurals name="sms_title_template">
        <item quantity="one">%1$s conversation non lue</item>
        <item quantity="other">%1$s conversations non lues</item>
    </plurals>
    <string name="sms_body_template">De : %1$s</string>

    <!-- Welcome strings -->
    <string name="welcome_title">Personnalisez votre DashClock</string>
    <string name="welcome_description">Vous voulez seulement voir la date et l\'heure ? Cochez
        la case juste au dessus et c\'est fait ! Mais pourquoi ne pas customiser DashClock avec 
        quelques <i>extensions</i> ?
        \n\nLes <b>extensions</b> vous affichent des informations pertinentes et en temps opportun, 
        à l\'intérieur même de DashClock ! Essayez-les en ajoutant quelques extensions ci-dessous.
    </string>

</resources>
