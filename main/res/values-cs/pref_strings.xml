<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Weather -->
    <string name="pref_weather_units_title">Jednotky teploty</string>
    <string-array name="pref_weather_units_display_names">
        <item>°F</item>
        <item>°C</item>
    </string-array>
    <string-array name="pref_weather_units_values">
        <item>f</item>
        <item>c</item>
    </string-array>

    <!-- Gmail -->
    <string name="pref_gmail_label_title">Zobrazit počet nepřečtených v popisku</string>
    <string-array name="pref_gmail_label_display_names">
        <item>Pošta</item>
        <item>Prioritní pošta</item>
    </string-array>
    <string-array name="pref_gmail_label_inbox_values">
        <item>i</item>
        <item>p</item>
    </string-array>

    <string name="pref_gmail_accounts_title">Zobrazit počet nepřečtených na účtech</string>
    <plurals name="pref_gmail_accounts_summary_template">
        <item quantity="one">
            Vybrán účet:
	          <xliff:g id="selected">%1$d</xliff:g> z <xliff:g id="total">%2$d</xliff:g>
        </item>
        <item quantity="other">
						Vybraných účtů:
            <xliff:g id="selected">%1$d</xliff:g> z <xliff:g id="total">%2$d</xliff:g>
        </item>
    </plurals>
</resources>
