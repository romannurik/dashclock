    <!-- Weather -->
    <string name="weather_extension_title">Wetter</string>
    <string name="weather_extension_description">
        Zeigt das aktuelle Wetter an Ihrem Standort an, darunter die aktuelle Temperatur, Wetterverhältnisse und Vorhersagen.
        Realisiert mit Yahoo! Weather.
    </string>
    <string name="title_weather_settings">Wetter-Einstellungen</string>
    <string name="temperature_template">%1$s°</string>
    <string name="later_forecast_template">Später: %1$s</string>
    <string name="no_weather_data">Keine Wetterdaten verfügbar</string>
    <string name="no_location_data">Keine Position verfügbar</string>

    <!-- Gmail -->
    <string name="gmail_extension_title">Gmail</string>
    <string name="gmail_extension_description">
        Zeigt die Anzahl Ihrer ungelesenen GoogleMail-Nachrichten im Posteingang an.
    </string>
    <string name="title_gmail_settings">Gmail-Einstellungen</string>
    <plurals name="gmail_title_template">
        <item quantity="other">%1$d ungelesene</item>
    </plurals>

    <!-- Next alarm -->
    <string name="next_alarm_extension_title">Nächster Alarm</string>
    <string name="next_alarm_extension_description">
        Zeigt informationen über den nächsten Alarm an, sofern einer geplant ist.
    </string>
    <string name="next_alarm">Nächster Alarm</string>

    <!-- Calendar -->
    <string name="calendar_extension_title">Kalender</string>
    <string name="calendar_extension_description">
        Zeigt Informationen über Ihren nächsten Termin an, sofern einer in den nächsten Stunden bevorsteht.
    </string>
    <string name="title_calendar_settings">Kalendereinstellungen</string>
    <plurals name="calendar_template_days">
        <item quantity="one">%1$s Tag</item>
        <item quantity="other">%1$s Tage</item>
    </plurals>
    <plurals name="calendar_template_hours">
        <item quantity="one">%1$s Stunde</item>
        <item quantity="other">%1$s Stunden</item>
    </plurals>
    <plurals name="calendar_template_mins">
        <item quantity="one">%1$s Minute</item>
        <item quantity="other">%1$s Minuten</item>
    </plurals>

    <!-- Missed calls -->
    <string name="missed_calls_extension_title">Verpasste Anrufe</string>
    <string name="missed_calls_extension_description">
        Zeigt die Anzahl verpasster Anrufe und den Namen der Anrufer an.
    </string>
    <plurals name="missed_calls_title_template">
        <item quantity="one">%1$s verpasster Anruf</item>
        <item quantity="other">%1$s verpasste Anrufe</item>
    </plurals>
    <string name="missed_calls_body_template">Von: %1$s</string>

    <!-- SMS + MMS -->
    <string name="sms_extension_title">SMS/ MMS</string>
    <string name="sms_extension_description">
        Zeigt die Anzahl ungelesener SMS/ MMS, sowie deren Absender an.
    </string>
    <plurals name="sms_title_template">
        <item quantity="one">%1$s ungelesene Nachricht</item>
        <item quantity="other">%1$s ungelesene Nachrichten</item>
    </plurals>
    <string name="sms_body_template">Von: %1$s</string>

    <!-- Welcome strings -->
    <string name="welcome_title">Passen Sie Ihre DashClock an!</string>
    <string name="welcome_description">Sie wollen nur die Zeit und das Datum sehen? Dann drücken Sie auf das Häkchen oben links! Aber warum die DashClock nicht mit einigen <i>Erweiterungen</i> anpassen?
        \n\n<b>Erweiterungen</b> zeigen Ihnen neue und relevante Ereignisse und Informationen direkt in der DashClock an!
        Probieren Sie einige Erweiterungen aus, indem Sie sie unten hinzufügen.
    </string>

