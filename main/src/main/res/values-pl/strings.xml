    <!-- Welcome strings -->
    <string name="welcome_title">Dostosuj DashClock</string>
    <string name="welcome_description">Chcesz wyświetlać tylko aktualny czas i datę? Zaznacz powyższe pole i gotowe! 
        Ale może chciałbyś dostosować DashClock używając kilku <i>rozszerzeń</i>?
        \n\n<b>Rozszerzenia</b> pokazują ci istotne i aktualne informacje wewnątrz DashClock!
        Wypróbuj je, dodając kilka rozszerzeń poniżej.
    </string>

